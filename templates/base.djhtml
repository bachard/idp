            <title>{% block title %}{{ title }} - Minecradt server monitoring{% endblock %}</title>
            <meta charset="utf-8">
            <link href="/static/css/bootstrap.min.css" rel="stylesheet">
        {% endblock %}
    </head>
    <body>
        <div class="container">
            <header class="page-header">
                {% block header %}
